{
  "kind": "collectionType",
  "collectionName": "shop_orders",
  "info": {
    "singularName": "shop-order",
    "pluralName": "shop-orders",
    "displayName": "shop_order",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "items": {
      "type": "component",
      "repeatable": true,
      "component": "items.items"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "pending",
        "approved",
        "delivered",
        "canceled"
      ],
      "default": "pending"
    },
    "total": {
      "type": "decimal"
    },
    "orderID": {
      "type": "customField",
      "options": {
        "uuid-format": "^[A-F0-9]{12}$"
      },
      "customField": "plugin::strapi-advanced-uuid.uuid"
    },
    "nationality": {
      "type": "string"
    },
    "address": {
      "type": "string"
    }
  }
}
